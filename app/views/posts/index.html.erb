<div>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>

  <% @posts.each do |post| %>
    <div><%= post.body %></div>
    <div><%= image_tag post.post_image.url(:medium) %></div>
    <% post.comments.each do |comment| %>
      <div><%= comment.body %></div>
      <div><%= comment.id %></div>
    <% end %>
  <% end %>

</div>
